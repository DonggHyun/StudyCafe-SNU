<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.KioskSNU.snu.service.SeatStatusService">
    <resultMap id="seatStatusResultMap" type="seatStatusAlias">
        <result column="seatNumber"             property="seatNumber"/>
        <result column="status"                 property="status"/>
    </resultMap>

    <select id="getAll" resultMap="seatStatusResultMap">
        select      *
        from        seatStatus;
    </select>
</mapper>